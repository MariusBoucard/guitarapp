{
  "name": "neckwanker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "dev": "vite",
    "build:vite": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build:vite": "vite build && electron-builder",
    "electron:build:win": "vite build && electron-builder --win",
    "electron:build:mac": "vite build && electron-builder --mac",
    "electron:build:linux": "vite build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps && npm run vst3:install",
    "postuninstall": "electron-builder install-app-deps",
    "vst3:install": "cd native/vst3-host && npm install",
    "vst3:build": "cd native/vst3-host && npm run build",
    "vst3:build-debug": "cd native/vst3-host && npm run build-debug",
    "vst3:clean": "cd native/vst3-host && npm run clean",
    "vst3:rebuild": "npm run vst3:clean && npm run vst3:build"
  },
  "main": "dist-electron/background.js",
  "dependencies": {
    "@coderline/alphatab": "^1.6.3",
    "core-js": "^3.45.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.3.1",
    "guitar-chords": "^1.2.0",
    "guitar-js": "^1.0.2",
    "jstuner-ui": "^0.1.4",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.3",
    "pitch-detector": "^1.1.3",
    "soundtouchjs": "^0.2.1",
    "vue": "^3.5.19",
    "vue-metronome": "^0.0.1",
    "vue-slider-component": "^3.2.24"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^10.4.0",
    "rollup": "^4.21.2",
    "vite": "^7.1.3",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-eslint-parser": "^10.2.0"
  },
  "extraFiles": [
    "src/assets",
    "public"
  ],
  "extraResources": [
    {
      "from": "public",
      "to": "./public",
      "filter": [
        "**/*"
      ]
    },
    {
      "from": "src/img",
      "to": "img",
      "filter": [
        "**/*"
      ]
    },
    "./src/img/**"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "build": {
    "appId": "com.MariusCompany.ChristmasTree",
    "productName": "Neck Wanker v2",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "public/**/*",
      "node_modules/fs-extra/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.png"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "public/icon.png"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "AppImage"
      ],
      "icon": "public/icon.png"
    }
  },
  "author": {
    "name": "Marius Boucard Bocciarelli",
    "email": "marius.boucard-bocciarelli@laposte.net"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
